package com.demo.model.Service;

import java.util.Map;

import org.springframework.beans.factory.annotation.Value;
import org.springframework.stereotype.Service;

import lombok.RequiredArgsConstructor;

@Service
@RequiredArgsConstructor
public class KarloApiServiceImpl implements KarloApiService{
	
	@Value("${api.kakao.key}")
	private String kakaoRestApikey;

	@Override
	public String makeAiImage(String prompt) {
		Map<String, String> kalroReqHeader = Map.of(
				"Authorization", "KakaoAK "+ kakaoRestApikey
		);
		
		String host = "api.kakaobrain.com";
		String genImgPath = "/v2/infernece/karlo/t21";
		
		Map<String, Object> requestMap = Map.of(
				"version", "v2.1",
				"prompt", prompt,
				"negative_prompt","low quality, low contrast, draft, amateur, cut off, cropped, frame",
				"width" , 768,
				"height", 768,
				"image_format","png",
				"image_quality",100
		);
		
		return null;
	}

}
