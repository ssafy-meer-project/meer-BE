<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.meer.model.dao.MissionDao">

	<select id="selectMissionById" resultType="Mission" parameterType="Map">
		SELECT * from mission
		WHERE user_id = #{userId} AND mission_id = #{missionId}
	</select>
	

	<select id="selectMission" resultType="Mission" parameterType="String">
		SELECT * from mission
		WHERE user_id = #{userId}
	</select>
	
	<delete id="deleteMission" parameterType="String">
		DELETE FROM mission
		WHERE user_id = #{userId}
	</delete>
	
</mapper>